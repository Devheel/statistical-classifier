{
    "name": "camspiers/statistical-classifier",
    "version": "1.0.0",
    "description": "A php implementation of a Naive Bayes statistical classifier",
    "keywords": ["classifier", "bayes"],
    "homepage": "https://github.com/camspiers/statistical-classifier",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Cam Spiers",
        "email": "camspiers@gmail.com"
    }],
    "require": {
        "php": ">=5.3.0",
        "maximebf/cachecache": "~1.0",
        "symfony/console": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/config": "~2.1",
        "camspiers/porter-stemmer": "~1.0",
        "camspiers/shared-dependency-injection": "~0.1",
        "psr/log": "~1.0",
        "monolog/monolog": "~1.4"
    },
    "require-dev": {
        "devster/ubench": "~1.1"
    },
    "autoload": {
        "psr-0": {"Camspiers": "src/"},
        "classmap": [ "config/" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
