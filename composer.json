{
    "name": "camspiers/statistical-classifier",
    "description": "A PHP implementation of a Naive Bayes statistical classifier, including a structure for building other classifier, multiple data sources and multiple caching backends",
    "keywords": ["classifier", "naive", "bayes", "statistical", "tokenizer", "normalizer", "porter", "stemer", "cache", "command-line"],
    "homepage": "https://github.com/camspiers/statistical-classifier",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Cam Spiers",
        "email": "camspiers@gmail.com"
    }],
    "require": {
        "php": ">=5.3.0",
        "maximebf/cachecache": "~1.0",
        "symfony/console": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/config": "~2.1",
        "camspiers/porter-stemmer": "~1.0",
        "camspiers/shared-dependency-injection": "~0.1",
        "psr/log": "~1.0",
        "monolog/monolog": "~1.4"
    },
    "require-dev": {
        "devster/ubench": "~1.1",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {"Camspiers": "src/"},
        "classmap": [ "config/" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
